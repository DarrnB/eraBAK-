RETURNF LOCALS

@장비변경슬롯선택(대상)
#DIM 대상
#DIM 선택장비

CALL SINGLE_DRAWLINE
CALL ICPRINT(@"현재 <%ANAME(대상)%>의 장비", "L", 칼라_주의)
CALL SINGLE_DRAWLINE
CALL 착용장비이미지(대상)

@장비변경종류선택(대상,슬롯종류)
#DIM 대상
#DIM 슬롯종류
#DIM FIRST_LINE
$타입선택루프
FIRST_LINE = LINECOUNT
CLEARLINE LINECOUNT - FIRST_LINE
CALL SINGLE_DRAWLINE
SETCOLOR 칼라_주석
PRINTL 장비 종류 선택
RESETCOLOR
CALL SINGLE_DRAWLINE
IF 슬롯종류 == 0
	PRINTBUTTON  @"[%"모자", 20, LEFT%]",0
	PRINTL
	PRINTBUTTON  @"[%"장갑", 20, LEFT%]",1
	PRINTL
	PRINTBUTTON  @"[%"신발", 20, LEFT%]",2
ELSEIF 슬롯종류 == 1
	PRINTBUTTON  @"[%"가방", 20, LEFT%]",0
	PRINTL
	PRINTBUTTON  @"[%"배지", 20, LEFT%]",1
	PRINTL
	PRINTBUTTON  @"[%"헤어핀", 20, LEFT%]",2
ELSEIF 슬롯종류 == 2
	PRINTBUTTON  @"[%"부적", 20, LEFT%]",0
	PRINTL
	PRINTBUTTON  @"[%"손목시계", 20, LEFT%]",1
	PRINTL
	PRINTBUTTON  @"[%"목걸이", 20, LEFT%]",2
ENDIF
PRINTL
IF 착용장비:대상:슬롯종류 >= 0
PRINTBUTTON @"[%"착용 해제", 20, LEFT%]", 998
PRINTL
ENDIF
SETCOLOR 칼라_선택불가
CALL SINGLE_DRAWLINE
RESETCOLOR
PRINTBUTTON @"[%"돌아온다", MAX_CHARANAME_LENGTH, LEFT%]", 999
PRINTL
INPUT 
$종류선택루프
LOCAL:1 = 0
IF RESULT == 0
ARG = (슬롯종류 * 3) + RESULT
CLEARLINE LINECOUNT - FIRST_LINE
CALL SINGLE_DRAWLINE
SETCOLOR 칼라_주석
PRINTFORMD  %장비타입(ARG)%   
PRINTFORML %타입설명(ARG)%
RESETCOLOR
CALL SINGLE_DRAWLINE
FOR COUNT, 0, 81
	LOCAL = COUNT % 9
	IF 슬롯종류 == 0
		SIF LOCAL != 0
		CONTINUE
	ELSEIF 슬롯종류 == 1
		SIF LOCAL != 3
		CONTINUE
	ELSEIF 슬롯종류 == 2
		SIF LOCAL != 6
		CONTINUE
	ENDIF
	IF 장비:COUNT <= 0
		LOCAL:1 ++
		IF LOCAL:1 >= 9
			PRINTW 사용 가능한 장비가 없습니다
			RETURN
		ENDIF
		CONTINUE
	ENDIF
	PRINTBUTTON @"[%장비이름(COUNT), 20, LEFT%]",COUNT
	PRINT    보유수: 
	PRINTFORML {장비:COUNT}
NEXT
ELSEIF RESULT == 1
ARG = (슬롯종류 * 3) + RESULT
CLEARLINE LINECOUNT - FIRST_LINE
CALL SINGLE_DRAWLINE
SETCOLOR 칼라_주석
PRINTFORMD  %장비타입(ARG)%   
PRINTFORML %타입설명(ARG)%
RESETCOLOR
CALL SINGLE_DRAWLINE
FOR COUNT, 0, 81
	LOCAL = COUNT % 9
	IF 슬롯종류 == 0
	SIF LOCAL != 1
	CONTINUE
	ELSEIF 슬롯종류 == 1
	SIF LOCAL != 4
	CONTINUE
	ELSEIF 슬롯종류 == 2
	SIF LOCAL != 7
	CONTINUE
	ENDIF
	IF 장비:COUNT <= 0
		LOCAL:1 ++
		IF LOCAL:1 >= 9
			PRINTW 사용 가능한 장비가 없습니다
			RETURN
		ENDIF
		CONTINUE
	ENDIF
	PRINTBUTTON @"[%장비이름(COUNT), 20, LEFT%]",COUNT
	PRINT    보유수: 
	PRINTFORML {장비:COUNT}
NEXT
ELSEIF RESULT == 2
ARG = (슬롯종류 * 3) + RESULT
CLEARLINE LINECOUNT - FIRST_LINE
CALL SINGLE_DRAWLINE
