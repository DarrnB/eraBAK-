FOR COUNT, 0, 81
	LOCAL = COUNT % 9
	IF 슬롯종류 == 0
	SIF LOCAL != 2
	CONTINUE
	ELSEIF 슬롯종류 == 1
	SIF LOCAL != 5
	CONTINUE
	ELSEIF 슬롯종류 == 2
	SIF LOCAL != 8
	CONTINUE
	ENDIF
	IF 장비:COUNT <= 0
		LOCAL:1 ++
		IF LOCAL:1 >= 9
			PRINTW 사용 가능한 장비가 없습니다
			RETURN
		ENDIF
		CONTINUE
	ENDIF
	PRINTBUTTON @"[%장비이름(COUNT), 20, LEFT%]",COUNT
	PRINT    보유수: 
	PRINTFORML {장비:COUNT}
