;-------------------------------------------------
;에로스테이타스 표시
;-------------------------------------------------
@SHOW_INFO_ERO_DESCRIPTION(대상)
#DIM 대상
CALL SINGLE_DRAWLINE
PRINTFORML 육체
CALL COLOR_LINE

CALL SHOW_INFO_ERO_DESCRIPTION_BODY(대상)

CALL SINGLE_DRAWLINE
PRINTFORML 감각
CALL COLOR_LINE

CALL SHOW_INFO_ERO_DESCRIPTION_SENSE(대상)

CALL SINGLE_DRAWLINE
PRINTFORML 성적 기호
CALL COLOR_LINE

CALL SHOW_INFO_ERO_DESCRIPTION_FETISH(대상)

CALL SINGLE_DRAWLINE
PRINTFORML 함락
CALL COLOR_LINE

CALL SHOW_INFO_ERO_DESCRIPTION_FALLEN(대상)

CALL SINGLE_DRAWLINE
PRINTFORML 특수한 함락
CALL COLOR_LINE

CALL SHOW_INFO_ERO_DESCRIPTION_SP_FALLEN(대상)

CALL SINGLE_DRAWLINE
PRINTFORML SLG 설명
CALL COLOR_LINE
TRYCCALLFORM SHOW_CHARA_EXPLANATION_{NO:(대상)}
CALL SINGLE_EMPTY_LINE()
CATCH
	PRINTFORML SLG 설명 없음　
	CALL SINGLE_EMPTY_LINE()
ENDCATCH

;-------------------------------------------------
;에로스테이타스 표시  육체
;-------------------------------------------------
@SHOW_INFO_ERO_DESCRIPTION_BODY(대상)
#DIM 대상
IF TALENT:대상:키스미경험
	PRINTFORML 키스를 했던 적이 없다
ELSE
	CALL COLOR_PRINT(SEXUAL_EXPERIENCE:대상:첫체험_키스 == "불명" ? "누군가" # SEXUAL_EXPERIENCE:대상:첫체험_키스, 칼라_핑크, "에게", GETDEFCOLOR())
	SIF SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_키스 != ""
		CALL COLOR_PRINT(SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_키스, 칼라_핑크, "로", GETDEFCOLOR())
	PRINTFORML 첫키스를 바쳤다
	IF SEXUAL_LAST_EXPERIENCE:대상:첫체험_키스 != ""
		CALL COLOR_PRINT("바로 옆에서는", GETDEFCOLOR(), SEXUAL_LAST_EXPERIENCE:대상:첫체험_키스 == "불명" ? "누군가" # SEXUAL_LAST_EXPERIENCE:대상:첫체험_키스, 칼라_핑크, "에게", GETDEFCOLOR())
		SIF SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_키스 != ""
			CALL COLOR_PRINT(SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_키스, 칼라_핑크, "로", GETDEFCOLOR())
		PRINTFORML 입맞춤하고 있다
	ENDIF
ENDIF

IF HAS_VAGINA(대상)
	IF TALENT:대상:처녀
		PRINTFORML 질 구멍의 순결을 유지하고 있다
	ELSE
		CALL COLOR_PRINT(SEXUAL_EXPERIENCE:대상:첫체험_처녀 == "불명" ? "누군가" # SEXUAL_EXPERIENCE:대상:첫체험_처녀, 칼라_핑크, "에게", GETDEFCOLOR())
		SIF SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_처녀 != ""
			CALL COLOR_PRINT(SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_처녀, 칼라_핑크, "로", GETDEFCOLOR())
		PRINTFORML 순결을 바쳤다
		IF SEXUAL_LAST_EXPERIENCE:대상:첫체험_처녀 != ""
			CALL COLOR_PRINT("바로 옆에서는", GETDEFCOLOR(), SEXUAL_LAST_EXPERIENCE:대상:첫체험_처녀 == "불명" ? "누군가" # SEXUAL_LAST_EXPERIENCE:대상:첫체험_처녀, 칼라_핑크, "와", GETDEFCOLOR())
			SIF SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_처녀 != ""
				CALL COLOR_PRINT(SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_처녀, 칼라_핑크, "로", GETDEFCOLOR())
			PRINTFORML 사귀고 있다
		ENDIF
	ENDIF
ENDIF

IF TALENT:대상:애널처녀
	PRINTFORML 엉덩이 구멍의 순결을 유지하고 있다
ELSE
	CALL COLOR_PRINT(SEXUAL_EXPERIENCE:대상:첫체험_애널처녀 == "불명" ? "누군가" # SEXUAL_EXPERIENCE:대상:첫체험_애널처녀, 칼라_핑크, "에게", GETDEFCOLOR())
	SIF SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_애널처녀 != ""
		CALL COLOR_PRINT(SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_애널처녀, 칼라_핑크, "로", GETDEFCOLOR())
	PRINTFORML 엉덩이 구멍의 첫경험을 바쳤다
	IF SEXUAL_LAST_EXPERIENCE:대상:첫체험_애널처녀 != ""
		CALL COLOR_PRINT("바로 옆에서는", GETDEFCOLOR(), SEXUAL_LAST_EXPERIENCE:대상:첫체험_애널처녀 == "불명" ? "누군가" # SEXUAL_LAST_EXPERIENCE:대상:첫체험_애널처녀, 칼라_핑크, "와", GETDEFCOLOR())
		SIF SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_애널처녀 != ""
			CALL COLOR_PRINT(SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_애널처녀, 칼라_핑크, "로", GETDEFCOLOR())
		PRINTFORML 사귀고 있다
	ENDIF
ENDIF

IF HAS_PENIS(대상)
	IF TALENT:대상:동정
		PRINTFORML 아직도 동정이다
	ELSE
		CALL COLOR_PRINT(SEXUAL_EXPERIENCE:대상:첫체험_동정 == "불명" ? "누군가" # SEXUAL_EXPERIENCE:대상:첫체험_동정, 칼라_핑크, "에게", GETDEFCOLOR())
		SIF SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_동정 != ""
			CALL COLOR_PRINT(SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_동정, 칼라_핑크, "로", GETDEFCOLOR())
		PRINTFORML 동정을 바쳤다
		IF SEXUAL_LAST_EXPERIENCE:대상:첫체험_동정 != ""
			CALL COLOR_PRINT("바로 옆에서는", GETDEFCOLOR(), SEXUAL_LAST_EXPERIENCE:대상:첫체험_동정 == "불명" ? "누군가" # SEXUAL_LAST_EXPERIENCE:대상:첫체험_동정, 칼라_핑크, "와", GETDEFCOLOR())
			SIF SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_동정 != ""
				CALL COLOR_PRINT(SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_동정, 칼라_핑크, "로", GETDEFCOLOR())
			PRINTFORML 사귀고 있다
		ENDIF
	ENDIF
ENDIF

IF IS_FEMALE(대상)
	SELECTCASE GET_BUSTSIZE(대상)
		CASE -2
			PRINTFORML 가슴에는 부푼 곳이 없고, 완전히 평평함
		CASE -1
			PRINTFORML 유방은 좀 작지만 형태를 갖추고 있다
		CASE 0
			PRINTFORML 유방은 딱 좋은 크기의 매력적인 형태를 하고 있다
		CASE 1
			PRINTFORML 풍만하고 부드러울 것 같은 유방이 눈길을 끈다
		CASE 2
			PRINTFORML 당장이라도 터질 듯한 곡선을 자랑하는 유방이 시선을 끌어당겨 자신에게 못을 박는다
	ENDSELECT
	SELECTCASE GET_HIPSIZE(대상)
		CASE -2
			PRINTFORML 엉덩이엔 살집이 없고, 완전히 평평함
		CASE -1
			PRINTFORML 엉덩이는 좀 작지만 형태를 갖추고 있다
		CASE 0
			PRINTFORML 엉덩이는 딱 좋은 크기의 매력적인 형태를 하고 있다
		CASE 1
			PRINTFORML 풍만하고 탄력있는 엉덩이가 눈길을 끈다
		CASE 2
			PRINTFORML 당장이라도 터질 듯이 풍만한 엉덩이가 시선을 끌어당겨 자신에게 못을 박는다
	ENDSELECT
	SIF TALENT:대상:모유체질
		CALL COLOR_PRINTL("유방에서는 모유가 분비되고 있다", 칼라_핑크)
	SIF TALENT:대상:각선미
		PRINTFORML 날씬하고 아름다운 다리가 이성을 매료한다
	SIF TALENT:대상:예쁜엉덩이
		PRINTFORML 꽉 조일 듯이 힙업된 아름다운 엉덩이가 이성을 매료한다
ENDIF

IF TALENT:대상:임신
	CALL ICPRINT(@"<\@ GET_SPERM_NAME(CFLAG:대상:아이의부친) == "불명" ? 부친 불명 # %GET_SPERM_NAME(CFLAG:(대상):아이의부친)% \@>의 아이를 품고 있다", "L", 칼라_핑크)
	SIF CFLAG:대상:행동불능상태 == 행동불능_임월
		CALL COLOR_PRINTL("임월을 맞이한 배가 크게 부풀어 있다", 칼라_핑크)
ENDIF 

FOR LOCAL, 0, 2
		CONTINUE
	IF LOCAL == 0
		SIF !HAS_VAGINA(대상)
			CONTINUE
		LOCAL:1 = TALENT:대상:Ｖ조임
		LOCALS = 여자구멍
	ELSE
		LOCAL:1 = TALENT:대상:Ａ조임
		LOCALS = 엉덩이 구멍
	ENDIF

	SELECTCASE LOCAL:1
		CASE IS >= 조임_꽉닫힘
			PRINTFORML %조사처리(LOCALS,"는")% 새끼 손가락조차 들어가지 못할 만큼 좁다
		CASE IS >= 조임_꽉조임
			PRINTFORML %조사처리(LOCALS,"는")% 새끼 손가락조차 단단히 조일 만큼 좁다
		CASE IS >= 조임_빡빡함
			PRINTFORML %조사처리(LOCALS,"는")% 손가락을 꼬옥꼬옥 단단히 조여 온다
		CASE IS >= 조임_명기
			PRINTFORML %조사처리(LOCALS,"는")% 한 번 물면 놓지 않는 명기다
		CASE IS >= 조임_보통
			PRINTFORML %LOCALS%의 조임은 평범하다
		CASE IS >= 조임_느슨한
			PRINTFORML %LOCALS%의 조임은 느슨하다
		CASE IS >= 조임_느슨느슨
			PRINTFORML %조사처리(LOCALS,"는")% 대단히 느슨해진 상태다
		CASE IS >= 조임_뻐끔뻐끔
			PRINTFORML 사용된 %조사처리(LOCALS,"는")% 뻐끔 열려 닫히지 않게 되었다
		CASE IS >= 조임_헐렁헐렁
			PRINTFORML 너무 사용된 나머지, %조사처리(LOCALS,"는")% 형태가 망가져 헐렁해져 버렸다
		CASEELSE
			PRINTFORML 너무 사용된 나머지, %조사처리(LOCALS,"는")% 원형을 떠올리지 못할 정도로 무너져 버렸다
	ENDSELECT
NEXT

SELECTCASE TALENT:대상:음모현재치
	CASE 음모_백보지
		PRINTFORML 음모는 제모되어 치부는 반질반질한 상태다
	CASE 음모_깔끔
		PRINTFORML 음모가 나지 않아 치부는 반질반질한 상태다
	CASE 음모_손질됨
		PRINTFORML 음모는 제대로 손질되어 있다
	CASE 음모_솜털
		PRINTFORML 치부에는 희미하게, 음모가 솜털과 함께 나 있다
	CASE 음모_살짝
		PRINTFORML 얇은 음모가 치부에 풀밭을 만들었다
	CASE 음모_표준
		PRINTFORML 음모는 평범한 모양과 형태를 갖추고 있다
	CASE 음모_많은
		PRINTFORML 음모가 평균보다 약간 많다
	CASE 음모_주렁주렁
		PRINTFORML 음모가 주렁주렁 무성하게 나 있다
	CASE 음모_무식하게
		PRINTFORML 무식할 정도로 무성하게 난 음모가 진한 수풀을 만들었다
	CASE 음모_강모
		PRINTFORML 굵은 음모가 밀림처럼 무성하게 나, 음부를 덮고 있다
	CASE 음모_항문에난털
		PRINTFORML 손질되지 않은 음모가 마구잡이로 자라 엉덩이까지 닿았다
	CASE 음모_하트형
		CALL COLOR_PRINTL("음모는 이성을 유혹하려는 듯 하트 모양을 갖추고 있다", 칼라_핑크)
	CASEELSE
		PRINTFORML 음모는 평범한 모양과 형태를 갖추고 있다
ENDSELECT

SELECTCASE TALENT:대상:겨드랑이털현재치
	CASE 겨드랑이털_제모완료
		PRINTFORML 털이 제모되어 겨드랑이가 반질반질한 상태다
	CASE 겨드랑이털_깔끔
		PRINTFORML 털이 나지 않아 겨드랑이가 반질반질한 상태다
	CASE 겨드랑이털_손질됨
		PRINTFORML 겨드랑이는 제대로 손질되어 있다
	CASE 겨드랑이털_솜털
		PRINTFORML 겨드랑이에는 희미하게, 겨드랑이털이 솜털과 함께 나 있다
	CASE 겨드랑이털_살짝
		PRINTFORML 얇은 털이 겨드랑이에 풀밭을 만들었다
	CASE 겨드랑이털_보통
		PRINTFORML 겨드랑이털은 평범한 모양과 형태를 갖추고 있다
	CASE 겨드랑이털_많은
		PRINTFORML 겨드랑이털이 평균보다 약간 많다
	CASE 겨드랑이털_주렁주렁
		PRINTFORML 겨드랑이털이 주렁주렁 무성하게 나 있다
	CASE 겨드랑이털_무식하게
		PRINTFORML 무식할 정도로 무성하게 난 겨드랑이털이 진한 수풀을 만들었다
	CASE 겨드랑이털_강모
		PRINTFORML 진한 겨드랑이털이 밀림처럼 무성하게 나, 겨드랑이를 가리고 있다
	CASE 겨드랑이털_밀림
		PRINTFORML 손질되지 않은 겨드랑이털이 마구잡이로 자라 겨드랑이를 비집고 나왔다
	CASEELSE
		PRINTFORML 겨드랑이털은 평범한 모양과 형태를 갖추고 있다
ENDSELECT


SIF IS_PIERCED(대상, 피어스_유두) || IS_PIERCED(대상, 피어스_클리토리스) || IS_PIERCED(대상, 피어스_페니스) || IS_PIERCED(대상, 피어스_음순)
	CALL COLOR_PRINTL("음탕한 부위에 피어스를 달고 있다", 칼라_핑크)

SIF IS_TATTOOED(대상, 문신_뺨) || IS_TATTOOED(대상, 문신_이마)
	CALL COLOR_PRINTL("얼굴 주위에 눈에 띄는 문신이 그려져 있다", 칼라_핑크)

SIF IS_TATTOOED(대상, 문신_가슴) || IS_TATTOOED(대상, 문신_비부) || IS_TATTOOED(대상, 문신_엉덩이)
	CALL COLOR_PRINTL("음탕한 부위에 문신이 그려져 있다", 칼라_핑크)

CALL SINGLE_EMPTY_LINE()

