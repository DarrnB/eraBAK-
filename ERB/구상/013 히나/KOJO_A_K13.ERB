	IF !(TCVAR:51 || TCVAR:52 || TCVAR:53)
		;PRINTFORMW 
	ENDIF
;기성 사실 Lv2(연인이 되었다)
ELSEIF CFLAG:201 < 4 && MARK:2 == 2
	CFLAG:201 = 4
	;행동 불능이라면 플래그만 세워 표시는 하지 않는다
	IF !(TCVAR:51 || TCVAR:52 || TCVAR:53)
		;PRINTFORMW 
	ENDIF
;기성 사실 Lv1(처음으로 키스했다)
ELSEIF CFLAG:201 < 3 && MARK:2 == 1
	CFLAG:201 = 3
	;행동 불능이라면 플래그만 세워 표시는 하지 않는다
	IF !(TCVAR:51 || TCVAR:52 || TCVAR:53)
		PRINTFORMW 
	ENDIF

;-------------------------------------------------
;종료시(2회째 이후)
;-------------------------------------------------
;행동 불능이라면 비표시
ELSE
	;기성 사실 Lv3 또한 연모
	IF MARK:2 == 3 && TALENT:연모
		;PRINTFORMW 
	;기성 사실 Lv3(섹스가 끝난 상태)
	ELSEIF MARK:2 == 3
		;PRINTFORMW 
	;기성 사실 Lv2(연인)
	ELSEIF MARK:2 == 2
		;PRINTFORMW 
	;기성 사실 Lv1(키스가 끝난 상태)
	ELSEIF MARK:2 == 1
		;PRINTFORMW 
	;기성 사실 Lv0로 연모
	ELSEIF MARK:2 == 0 && TALENT:연모
		;PRINTFORMW 
	ELSE
		PRINTFORMW 「조심히 들어가.」
		PRINTFORMW 　히나는 방에서 떠나는 %조사처리(ANAME(MASTER),"을")% 배웅했다.
	ENDIF
ENDIF

